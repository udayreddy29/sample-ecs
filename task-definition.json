{
    "family": "registry",
    "containerDefinitions": [
        {
            "name": "registry",
            "image": "363128713288.dkr.ecr.us-west-1.amazonaws.com/sample-ecs",
            "cpu": 128,
            "memoryReservation": 128,
            "portMappings": [
                {
                    "containerPort": 80,
                    "protocol": "tcp",
                    "hostPort": 8080
                }
            ],
            "essential": true
        }
    ]
}
